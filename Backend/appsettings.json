{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
